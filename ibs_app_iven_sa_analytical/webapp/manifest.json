{
  "_version": "1.53.0",
  "sap.app": {
    "id": "com.ibs.ibsappivensaanalytical",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.1",
      "toolsId": "691858fa-96ab-4947-beb7-36b92cf20d68"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZIVN_PO_CONFIRMATION_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },    
      "checkServiceAvailibiltyService": {
        "uri": "/odata/v4/addtional-process/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/doc-info-ext/metadata.xml",
          "odataVersion": "4.0"
        }   
      },
      "appSaInfoService": {
        "uri": "/odata/v4/app-sa-info/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/doc-info-ext/metadata.xml",
          "odataVersion": "4.0"
        }   
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "componentUsages": {
    },
    "dependencies": {
      "minUI5Version": "1.114.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ibs.ibsappivensaanalytical.i18n.i18n"
        }
      },
      "oDataModel2": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "products": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/products.json"
      },
      "purchase": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/purchase.json"
      },
      "managed": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/managed.json"
      },
      "checkServiceAvailibilty": {
        "preload": true,
        "dataSource": "checkServiceAvailibiltyService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "appSaInfo": {
        "preload": true,
        "dataSource": "appSaInfoService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "clearTarget": true,
        "bypassed": {
            "target": "TargetServiceMsg"
        },
        "viewPath": "com.ibs.ibsappivensaanalytical.view",
        "controlAggregation": "pages",
        "controlId": "myPage"
      },
      "routes": [
        {
          "name": "Default",
          "pattern": "",    
          "target": [
            "TargetBase"
          ]
        },
        {
          "name": "Master",
          "pattern": "Master",
          "target": [
            "Home"
          ]
        },
        {
          "name": "Page1",
          "pattern": "Page1",
          "target": [
            "Page1"
          ]
        },
        {
          "name": "Page2",
          "pattern": "Page2",
          "target": [
            "Page2"
          ]
        },
        {
          "name": "iVenMaster",
          "pattern": "iVenMaster",    
          "target": [
            "TargetiVenMaster"  
          ]
        }
      ],
      "targets": {
        "TargetBase": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "App",
          "viewName": "App"
        },
        "Home": {
          "viewType": "XML",
          "viewName": "Master",
          "controlAggregation": "pages"
        },
        "Page1": {
          "viewType": "XML",
          "viewName": "Page1",
          "controlAggregation": "pages"
        },
        "Page2": {
          "viewType": "XML",
          "viewName": "Page2",
          "controlAggregation": "pages"
        },
        "TargetiVenMaster":{
          "viewType": "XML",
          "viewName": "iVenMaster"          
        },
        "TargetServiceMsg":{
          "viewType": "XML",
          "viewName": "ServiceMsg"               
        }
      }
    },
    "rootView": {
      "viewName": "com.ibs.ibsappivensaanalytical.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}