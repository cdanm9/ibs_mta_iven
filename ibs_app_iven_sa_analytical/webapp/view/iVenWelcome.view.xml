<mvc:View controllerName="com.ibs.ibsappivensaanalytical.controller.iVenWelcome"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="idWelcomePage" title="{appWelcomePage>/0/APP_NAME}">    
        <content>
            <Carousel pages="{
				path: 'appWelcomePage>/'
			}" class="sapUiContentPadding" loop="true">   
                <Image densityAware="true" src="{appWelcomePage>LOGO_URL}" width="auto" height="auto" >
                <detailBox>
						<LightBox>
							<LightBoxItem
								imageSrc="{appWelcomePage>LOGO_URL}"
								alt="sap-icon://error"   
								title="Welcome"
								subtitle="iVen" />  
						</LightBox>
					</detailBox>       
                </Image>    
            </Carousel> 
        </content>
    </Page>
</mvc:View>  